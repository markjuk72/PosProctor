      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "__name__": true,
              "job": true,
              "instance": true,
              "Time": true
            },
            "indexByName": {
              "store": 0,
              "group": 1,
              "brand": 2
            },
            "renameByName": {
              "store": "Store",
              "group": "Group", 
              "brand": "Brand",
              "ip": "IP",
              "Value #A": "Controller",
              "Value #B": "Pumps",
              "Value #C": "DCRs", 
              "Value #D": "Fuel Signs",
              "Value #E": "Loyalty"
            }
          }
        },
        {
          "id": "sortBy",
