          "format": "table",
          "instant": true
        },
        {
          "refId": "D",
          "expr": "min by (store, group, ip, brand) (posproctor_price_display_status{group=~'$group', brand=~'$brand'})",
          "format": "table",
          "instant": true
        },
        {
          "refId": "E",
          "expr": "min by (store, group, ip, brand) (posproctor_loyalty_fep_status{group=~'$group', brand=~'$brand'})",
          "format": "table",
          "instant": true
        }
